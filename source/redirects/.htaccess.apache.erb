# MOVE PERMANENTLY
<% data.htaccess.short.redirect.each do |old, new| %>
Redirect 301 <%= old %> <%= data.htaccess.web.url %><%= new %>
<% end %>

# REDIRECT JOURNAL
Options +FollowSymLinks
RewriteEngine On
RewriteCond %{REQUEST_URI} ^<%= data.htaccess.short.web.journal %>/(.*)$
RewriteRule ^(.*) <%= data.htaccess.web.url %><%= data.htaccess.web.journal %>/%1 [R=302,NC]

# REDIRECT CASE STUDIES
Options +FollowSymLinks
RewriteEngine On
RewriteCond %{REQUEST_URI} ^<%= data.htaccess.short.web.case_study %>/(.*)$
RewriteRule ^(.*) <%= data.htaccess.web.url %><%= data.htaccess.web.case_study %>/%1 [R=302,NC]